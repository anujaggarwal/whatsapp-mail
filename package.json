    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "pg": "^8.18.0",
